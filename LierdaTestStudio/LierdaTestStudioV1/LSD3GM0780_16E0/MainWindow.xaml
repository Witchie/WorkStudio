<dx:ThemedWindow
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" 
        x:Class="LSD3GM0780_16E0.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LSD3GM0780_16E0"
        mc:Ignorable="d" Loaded="ThemedWindow_Loaded"    
        xmlns:dxmvvm="http://schemas.devexpress.com/winfx/2008/xaml/mvvm"
        Title="MainWindow" Height="450" Width="800">
    <dxmvvm:Interaction.Behaviors>
        <dx:DXSplashScreenService ShowSplashScreenOnLoading="True"
                                   SplashScreenType="{x:Type local:SartupView}" />
        <dxmvvm:EventToCommand />
    </dxmvvm:Interaction.Behaviors>
</dx:ThemedWindow>
